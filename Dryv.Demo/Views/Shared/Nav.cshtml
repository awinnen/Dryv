@using Dryv.Demo.Nav
@inject NavCollector NavCollector
@{
    var menu = new[]
    {
Menu.Introduction,
Menu.Documentation,
Menu.ExtendingDryv,
Menu.Examples,
};
    var items = NavCollector.GetNavStructure(this.ViewContext);
}

@{
    foreach (var menuItem in menu)
    {
        @Html.Partial("NavItem", items[menuItem])
    }
}
